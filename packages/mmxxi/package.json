{
  "name": "mmxxi",
  "version": "1.0.0",
  "description": "2021 advent of code",
  "main": "dist/index.js",
  "scripts": {
    "dev:run": "nodemon --exec node --require ts-node/register --require tsconfig-paths/register --require dotenv/config src/index.ts",
    "dev": "concurrently npm:typecheck:watch npm:dev:run --raw",
    "debug": "nodemon --exec node --inspect-brk=0.0.0.0:9229 --require ts-node/register --require tsconfig-paths/register --require dotenv/config src/index.ts",
    "add": "npx lerna add --scope=@forgescript/cli",
    "add:dev": "npx lerna add -D --scope=@forgescript/cli",
    "build:": "tsc -b",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "devDependencies": {
    "@swc/core": "1.2.119",
    "@swc/helpers": "0.3.2",
    "@swc/jest": "0.2.12",
    "@theomessin/ts-builder": "^0.1.8",
    "@types/debug": "4.1.7",
    "@types/jest": "27.0.3",
    "@types/moo": "^0.5.5",
    "@types/node": "16.11.12",
    "concurrently": "6.4.0",
    "fp-ts": "2.11.5",
    "jest": "27.4.4",
    "nodemon": "2.0.15",
    "regenerator-runtime": "0.13.9",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "tsconfig-paths": "3.12.0",
    "tslib": "2.3.1"
  },
  "keywords": [],
  "author": ""
}
